; Loads at 0 in user memory. Takes a second
; or two to execute, providing a machine cycles
; per second estimator.

	lsi r1, 0
	lsi r2, 50

inner:
	adi r1, r1, -1
	beq r1, r0, outer
	beq r0, r0, inner

outer:
	adi r2, r2, -1
	beq r2, r0, done
	; brk ; uncomment to produce r2 number of register dumps
	beq r0, r0, inner

done:
	hlt
